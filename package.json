{
  "name": "msg-viewer",
  "version": "1.0.0",
  "description": "A viewer for Outlook .msg files",
  "main": "electron/main.js",
  "scripts": {
    "dev": "node scripts/copy-pdf-worker.js && vite --host",
    "build": "node scripts/copy-pdf-worker.js && vite build",
    "serve": "vite preview",
    "electron-dev": "NODE_ENV=development concurrently -k \"cross-env BROWSER=none npm run dev\" \"wait-on tcp:5173 && electron .\"",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "npm run build",
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "@headlessui/react": "^2.2.0",
    "@kenjiuno/msgreader": "^1.4.0",
    "node-addon-api": "^8.3.0",
    "node-gyp": "^11.0.0",
    "pdfjs-dist": "3.11.174",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^28.1.0",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "vite": "^5.0.12",
    "wait-on": "^7.2.0"
  }
}
